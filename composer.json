{
	"name": "fromholdio/silverstripe-listings",
	"type": "silverstripe-vendormodule",
	"description": "A SilverStripe module providing foundation for pages with listed records.",
	"license": "BSD-3-Clause",
	"keywords": ["silverstripe", "pagination"],
	"homepage": "https://github.com/fromholdio/silverstripe-listings",
	"support": {
		"issues": "https://github.com/fromholdio/silverstripe-listings/issues"
	},
	"authors": [{
		"name": "Luke Fromhold",
		"homepage": "https://fromhold.io"
	}],
    "require": {
        "php": ">=5.6",
        "silverstripe/vendor-plugin": "^1.0",
        "silverstripe/framework": "^4.0",
        "fromholdio/silverstripe-commonancestor": "^1.0",
        "symbiote/silverstripe-gridfieldextensions": "^3.0"
    },
    "suggest": {
        "fromholdio/silverstripe-fulltext-innodb": "^1.0",
        "fromholdio/silverstripe-fulltext-filters": "^1.0"
    },
	"autoload": {
		"psr-4": {
			"Fromholdio\\Listings\\": "src/"
		}
	},
    "extra": {
        "expose": [
            "client"
        ]
    },
	"prefer-stable": true,
	"minimum-stability": "dev"
}
